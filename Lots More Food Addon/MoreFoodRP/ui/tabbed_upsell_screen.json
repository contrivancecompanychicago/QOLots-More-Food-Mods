{
	"namespace": "tabbed_upsell",
	"morefood_panel": {
		"type": "stack_panel",
		"orientation": "horizontal",
		"size": ["100%",256],
		"offset": [4,4],
		"controls": [
			{"1": {"type": "image","texture": "textures/ui/banner.png","size": [400,200]}},
			{
				"2":{
					"type": "stack_panel",
					"size": ["fill","100%"],
					"controls": [
						{"b": {"type": "label","text": "©2022 §b@Legopitstop\n§rLegopitstop Common License.\nsee LICENSE for more\n \n§6Meet the team"}},
						{"c": {"type": "label","font_scale_factor": 0.8,"text": "§eProgrammer:\n §rLegopitstop - https://legopitstop.weebly.com/profile.html"}},
						{"d": {"type": "label","font_scale_factor": 0.8,"text": "§eTextures artest:\n §rSuperSonic - ??"}},
						{"e": {"type": "label","font_scale_factor": 0.8,"text": "§eLegacy artest:\n §rSircraftypants - https://twitter.com/CraftypantsSir"}},
						{"f": {"type": "label","text": "\n\n§6Other Links"}},
						{"g": {"type": "label","font_scale_factor": 0.8,"text": "§eDownload:\n §rhttps://mcpedl.com/lots-more-food-bedrock-addon"}},
						{"h": {"type": "label","font_scale_factor": 0.8,"text": "§eBug Report:\n §rhttps://legopitstop.weebly.com/bug-report-more-food.html"}},
						{"i": {"type": "label","font_scale_factor": 0.8,"text": "§eRecipes:\n §rhttps://legopitstop.weebly.com/recipes-morefood.html"}},
						{"j": {"type": "label","font_scale_factor": 0.8,"text": "§eWiki:\n §rhttps://more-food.fandom.com/wiki/"}},
						{
							"k":{
								"type": "stack_panel",
								"size": ["fill","100%"],
								"controls": [
									{
										"paragraph_1@how_to_play_common.paragraph": {
										  "$text": "update.changelog.0"
										}
									},
									{
										"paragraph_2@how_to_play_common.paragraph": {
										  "$text": "update.changelog.1"
										}
									},
									{
										"paragraph_3@how_to_play_common.paragraph": {
										  "$text": "update.changelog.2"
										}
									},
									{
										"paragraph_4@how_to_play_common.paragraph": {
										  "$text": "update.changelog.3"
										}
									},
									{
										"paragraph_5@how_to_play_common.paragraph": {
										  "$text": "update.changelog.4"
										}
									},
									{
										"paragraph_6@how_to_play_common.paragraph": {
										  "$text": "update.changelog.5"
										}
									},
									{
										"paragraph_7@how_to_play_common.paragraph": {
										  "$text": "update.changelog.more"
										}
									}
								]
							}
						}

					]
				}
			}
		]
	},
	"scroll@common.scrolling_panel": {
		"$show_background": false,
		"$scrolling_content": "tabbed_upsell.morefood_panel"
	},
	"tabbed_upseel_screen_panel@common_dialogs.main_panel_no_buttons": {
		"size": ["80%","80%"],
		"$text_name": "§6More Food §eInfo",
		"$child_control": "tabbed_upsell.scroll"
	}
}