						{"c2": {"type": "label","text": "changelog.2"}},
						{"c3": {"type": "label","text": "changelog.3"}},
						{"c4": {"type": "label","text": "changelog.4"}},
						{"c5": {"type": "label","text": "changelog.5"}}

					]
				}
			}
		]
