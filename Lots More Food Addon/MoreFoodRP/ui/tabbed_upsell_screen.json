{
	"namespace": "tabbed_upsell",

	"morefood_panel": {
		"type": "stack_panel",
		"orientation": "horizontal",
		"size": ["100%",256],
		"offset": [4,4],
		"controls": [
			{
				"r":{
					"type": "stack_panel",
					"size": [132,"100%"],
					"controls": [
						{"1": {"type": "image","texture": "food_icon","size": [128,128]}},
						{"2": {"type": "panel","size": [4,4]}},
						{"3": {"type": "label","font_scale_factor": 1.5,"text": "§6More Food"}},
						{"4": {"type": "label","font_scale_factor": 1.5,"text": "update.version"}},
						{"5": {"type": "label","font_scale_factor": 1.2,"text": "update.changelog"}}
					]
				}
			},
			{
				"l":{
					"type": "stack_panel",
					"size": ["fill","100%"],
					"controls": [
						{"a": {"type": "label","font_scale_factor": 1.5,"text": "§6More Food Add-On \n§efor §6Bedrock Edition\n "}},
						{"b": {"type": "label","text": "©2021 §b@Legopitstop\n§rLegopitstop Common License.\nsee LICENSE for more\n \n§6URL"}},
						{"c": {"type": "label","font_scale_factor": 0.8,"text": "§eOfficial Download:\n §rmcpedl.com/lots-more-food-bedrock-addon"}},
						{"d": {"type": "label","font_scale_factor": 0.8,"text": "§eLegopitstop:\n §rlegopitstop.weebly.com/media.html"}},
						{"e": {"type": "label","font_scale_factor": 0.8,"text": "§eSir_craftypants:\n §rtwitter.com/CraftypantsSir"}},
						{ "padding_1@how_to_play_common.padding": {} },
						{
							"paragraph_1@how_to_play_common.paragraph": {
							  "$text": "update.changelog.0"
							}
						},
						{
							"paragraph_2@how_to_play_common.paragraph": {
							  "$text": "update.changelog.1"
							}
						},
						{
							"paragraph_3@how_to_play_common.paragraph": {
							  "$text": "update.changelog.2"
							}
						},
						{
							"paragraph_4@how_to_play_common.paragraph": {
							  "$text": "update.changelog.3"
							}
						},
						{
							"paragraph_5@how_to_play_common.paragraph": {
							  "$text": "update.changelog.4"
							}
						},
						{
							"paragraph_6@how_to_play_common.paragraph": {
							  "$text": "update.changelog.5"
							}
						},
						{
							"paragraph_7@how_to_play_common.paragraph": {
							  "$text": "update.changelog.more"
							}
						}
					]
				}
			}
		]
	},
	"scroll@common.scrolling_panel": {
		"$show_background": false,
		"$scrolling_content": "tabbed_upsell.morefood_panel"
	},
	"tabbed_upseel_screen_panel@common_dialogs.main_panel_no_buttons": {
		"size": ["80%","80%"],
		"$text_name": "§6More Food §eInfo",
		"$child_control": "tabbed_upsell.scroll"
	}
}